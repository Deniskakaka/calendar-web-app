  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.24.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "core-js": "^3.6.1"
  },
  "bugs": {
    "url": "https://github.com/DereviankoViacheslav/calendar-web-app/issues"
